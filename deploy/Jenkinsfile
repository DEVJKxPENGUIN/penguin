pipeline {
    agent any
    stages {
        stage('build Jar') {
            steps {
                echo 'Building..'
                echo "${params.role}"
                sh "./gradlew ${params.role}:bootJar"
                sh "ls -la ./${params.role}/build/libs"
            }
        }
        stage('build Docker') {
            steps {
                echo 'Building Docker..'
            }
        }
    }
}